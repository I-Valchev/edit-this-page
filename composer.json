{
  "name": "ivovalchev/edit-this-page",
  "description": "Bolt 4 extension to add Edit page button for Editors on frontend pages",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivo Valchev",
      "email": "me@ivovalchev.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "twig/twig": "^2.12 | ^3.0"
  },
  "require-dev": {
    "bolt/core": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "IvoValchev\\EditThisPageExtension\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "entrypoint": "IvoValchev\\EditThisPageExtension\\Extension",
    "screenshots": [
      "screenshots/button.png"
    ]
  }
}
